repeat(5)
{
    if instance_exists(enemyid[i]) or (enemyid[i] = 0) or (enemyid[i] = noone)
    or obj_mariob.phase &lt; 0 or obj_luigib.phase &lt; 0
        {}
    else
        spd = enemyid[i].spd;
    i += 1;
}

//Determine who goes first
if global.Mspd &lt; spd or global.Lspd &lt; spd
{
