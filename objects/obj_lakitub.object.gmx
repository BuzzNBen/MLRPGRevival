    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>enemynum = 0; //the number in obj_battlecontrol's variable enemyid

if id = obj_battlecontrol.enemyid[1]
    enemynum = 1;
if id = obj_battlecontrol.enemyid[2]
    enemynum = 2;
if id = obj_battlecontrol.enemyid[3]
    enemynum = 3;
if id = obj_battlecontrol.enemyid[4]
    enemynum = 4;
if id = obj_battlecontrol.enemyid[5]
    enemynum = 5;

if global.Mdead = 1 &amp;&amp; global.Ldead = 1 or dead = 1
    exit
else
{
    if x = stancex &amp;&amp; y = stancey
    {
        if global.Mdead = 0.5 &amp;&amp; obj_mariob.sprite_index = spr_mariodie
            exit;
        if global.Ldead = 0.5 &amp;&amp; obj_luigib.sprite_index = spr_luigidie
            exit;
        if global.Mdead = 1 &amp;&amp; global.Ldead = 1
            exit;
    }
    if hurt = 1
        hurttime += -0.2;
    if stance = 1
    {
        sprite_index = spr_lakitustance;
        image_xscale = 1;
    }
    /*if stance = 2
        sprite_index = spr_lakituattack;*/
    if hurttime &lt;= 0
    {
        hurt = 0;
        hurttime = 10;
    }
    if hurt = 1
        sprite_index = spr_lakituhurt;
    if hurt = 0 &amp;&amp; stance = 1
        sprite_index = spr_lakitustance;
    
    if x = stancex &amp;&amp; y = stancey
        stance = 1;
    if moveto = 0
        mp_linear_step(stancex,stancey,1.5,0); //Return to starting position
    if moveto = 1 //Attack Variant A (attack Mario)
    {
        mp_linear_step(240,obj_mariob.y+24,1.5,0); //Prepare to attack Mario
        sprite_index = spr_lakitustance;
        stance = 0;
        obj_mariob.stance = 4; //Battle stance
        obj_luigib.stance = 4; //Battle stance
    }
    if x = 240 &amp;&amp; y = obj_mariob.y+24 &amp;&amp; moveto = 1 //Ready to attack Mario
    {
        sprite_index = spr_lakituattack;
        stance = 2;
        moveto = 2;
    }
    if moveto = 2 &amp;&amp; sprite_index = spr_lakituattack &amp;&amp; image_index &gt;= 2
    {
        ID = instance_create(x+8,y,obj_lakituball); //Throw Lakitu Ball
        with ID target = 0;
        with ID zgravityon = 1; //Throw the ball
        moveto = -1;
    }
    if moveto = 3 //Attack Variant A (attack Luigi)
    {
        mp_linear_step(240,obj_luigib.y-24,1.5,0); //Prepare to attack Luigi
        sprite_index = spr_lakitustance;
        stance = 0;
        obj_mariob.stance = 4; //Battle stance
        obj_luigib.stance = 4; //Battle stance
    }
    if x = 240 &amp;&amp; y = obj_luigib.y-24 &amp;&amp; moveto = 3 //Ready to attack Luigi
    {
        sprite_index = spr_lakituattack;
        stance = 2;
        moveto = 4;
    }
    if moveto = 4 &amp;&amp; sprite_index = spr_lakituattack &amp;&amp; image_index &gt;= 2
    {
        ID = instance_create(x+8,y,obj_lakituball); //Throw Lakitu Ball
        with ID target = 1;
        with ID zgravityon = 1; //Throw the ball
        moveto = -1;
    }
    if moveto = 5 //Attack Variant B (check if other spinies exist)
    {
        if obj_battlecontrol.enemyid[1] = noone || obj_battlecontrol.enemyid[2] = noone
        || obj_battlecontrol.enemyid[4] = noone || obj_battlecontrol.enemyid[5] = noone
            moveto = 6;
        else
            moveto = choose(1,3);
    }
    if moveto = 6 //Attack Variant B (prepare to throw spiny)
    {
        mp_linear_step(xstart-66,y,1.5,0); //Prepare to throw
        sprite_index = spr_lakitustance;
        stance = 0;
        obj_mariob.stance = 4; //Battle stance
        obj_luigib.stance = 4; //Battle stance        
    }
    if x = xstart-66 &amp;&amp; moveto = 6 //Throwing spiny
    {
        sprite_index = spr_lakituattack;
        stance = 2;
        moveto = 7;
    }
    if moveto = 7 &amp;&amp; sprite_index = spr_lakituattack &amp;&amp; image_index &gt;= 2
    {
        ID = instance_create(x+8,y,obj_spinyb); //Throw Lakitu Ball
        with ID stance = 2;
        with ID sprite_index = spr_spinyfall;
        //Set the enemyid and prioritize in order: 2,4,1,5
        if !instance_exists(obj_battlecontrol.enemyid[2])
        {
            with ID obj_battlecontrol.enemyid[2] = id;
            with ID stancex = obj_battlecontrol.x
            with ID stancey = obj_battlecontrol.y-32
        }
        else if !instance_exists(obj_battlecontrol.enemyid[4])
        {
            with ID obj_battlecontrol.enemyid[4] = id;
            with ID stancex = obj_battlecontrol.x
            with ID stancey = obj_battlecontrol.y+32
        }
        else if !instance_exists(obj_battlecontrol.enemyid[1])
        {
            with ID obj_battlecontrol.enemyid[1] = id;
            with ID stancex = obj_battlecontrol.x+32
            with ID stancey = obj_battlecontrol.y-48
        }
        else if !instance_exists(obj_battlecontrol.enemyid[5])
        {
            with ID obj_battlecontrol.enemyid[5] = id;
            with ID stancex = obj_battlecontrol.x+32
            with ID stancey = obj_battlecontrol.y+48
        }
        with ID zgravityon = 1; //Make it airborn
        with ID z = obj_lakitub.z;
        with ID ground = 0;
        with ID zspeed = 5; //Determines speed of fall
        with ID zgravity = -1;
        moveto = -1;
    }
    
    enemyahead = 0; //enemyahead is just a variable used in coding, don't worry about it
    if turndone = 1 &amp;&amp; global.turn = 2
    {
        if enemynum &lt; 5
        {
            do 
            {
                enemyahead += 1;
                nextid = obj_battlecontrol.enemyid[enemynum+enemyahead];
            }
            until (instance_exists(nextid) || enemynum+enemyahead = 5)
            if instance_exists(nextid)
            {
                if nextid.moveto = 0 &amp;&amp; nextid.turndone = 0
                {
                    target = floor(random(6)); //Choose whether to attack Mario or Luigi
                    if global.Mdead &gt; 0
                        target = 1;
                    if global.Ldead &gt; 0
                        target = 0;
        
                    if target = 0 || target = 3
                        nextid.moveto = 1; //Move to Mario
                    if target = 1 || target = 4
                        nextid.moveto = 3; //Move to Luigi
                    if target = 2 || target &gt;= 5
                        nextid.moveto = 5; //Throw a spiny
                }
            }
        }
        if enemynum+enemyahead = 5 &amp;&amp; x = xstart &amp;&amp; y = ystart &amp;&amp; !instance_exists(obj_lakituball)
        {
            if global.Lspd &gt; global.Mspd &amp;&amp; global.turn &lt;&gt; 1
            {
                obj_luigib.turnnext = 0;
                obj_luigib.phase = 1;
                obj_mariob.turnnext = 1;
                obj_mariob.stance = 1;
                global.turn = 1;
            }
            else
            {
                obj_mariob.turnnext = 0;
                obj_luigib.turnnext = 1;
                obj_luigib.stance = 1;
                global.turn = 0;
            }
            turndone = 0;
        }
    }

    if turndone = 2 &amp;&amp; !instance_exists(obj_lakituball)
        turndone = 1;
    
    if global.turn != 2
        turndone = 0;

    if hp &lt;= 0 //Enemy dies
    {
        global.stunned = true;
        audio_play_sound(snd_enemy_dead,0,0);
        global.experience += experience;
        global.coinsearned += coins;
        event_user(0);
    }
}
</string>
          </argument>
        </arguments>
      </action>
