    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw background, textbox and text

/*
//Draw the screenshot if it exists
if (background_exists(back)) {

    //Disable alpha blending
    draw_enable_alphablend(0);
    
    //Draw screenshot
    draw_background(back,view_xview[0],view_yview[0]);
    
    //Enable alpha blending
    draw_enable_alphablend(1);
}
*/

//Animate
anim += 0.1;

//Set alpha
draw_set_alpha(1);

//Set colour of center of message box
switch (box)
{
    case 0:
        draw_set_colour(make_colour_rgb(254,254,242));
    break;
    case 1:
        draw_set_colour(make_colour_rgb(29,46,89));
    break;
    case 2:
        draw_set_colour(make_colour_rgb(254,254,242));
    break;
}
 


//Draw speech balloon
switch (box)
{
    case 0:
        draw_sprite(spr_dialog, 0, tX, tY); //Top-Left corner
        draw_sprite(spr_dialog, 2, tX + width + padding, tY); //Top-Right corner
        draw_sprite(spr_dialog, 4, tX + width + padding, tY + height + padding); //Bottom-Right corner
        draw_sprite(spr_dialog, 6, tX, tY + height + padding); //Bottom-Left corner    
    break;
    case 1:
        draw_sprite(spr_menu_dialog, 0, tX, tY); //Top-Left corner
        draw_sprite(spr_menu_dialog, 2, tX + width + padding, tY); //Top-Right corner
        draw_sprite(spr_menu_dialog, 4, tX + width + padding, tY + height + padding); //Bottom-Right corner
        draw_sprite(spr_menu_dialog, 6, tX, tY + height + padding); //Bottom-Left corner    
    break;
    case 2:
        draw_sprite(spr_exclamation_dialog, 0, tX, tY); //Top-Left corner
        draw_sprite(spr_exclamation_dialog, 2, tX + width + padding, tY); //Top-Right corner
        draw_sprite(spr_exclamation_dialog, 4, tX + width + padding, tY + height + padding); //Bottom-Right corner
        draw_sprite(spr_exclamation_dialog, 6, tX, tY + height + padding); //Bottom-Left corner    
    break;
}
 


//Draw hor. borders
for (var i=0; i&lt;width/8; i++;) {

    switch (box)
    {
        case 0:
            draw_sprite(spr_dialog, 1, tX + 8 + 8*i, tY);
            draw_sprite(spr_dialog, 5, tX + 8 + 8*i, tY + height + padding);
        break;
        case 1:
            draw_sprite(spr_menu_dialog, 1, tX + 8 + 8*i, tY);
            draw_sprite(spr_menu_dialog, 5, tX + 8 + 8*i, tY + height + padding);
        break;
        case 2:
            draw_sprite(spr_exclamation_dialog, 1, tX + 8 + 8*i, tY);
            draw_sprite(spr_exclamation_dialog, 5, tX + 8 + 8*i, tY + height + padding);
        break;
    }
     
}

//Draw ver. borders
for (var j=0; j&lt;height/8; j++;) {

    switch (box)
    {
        case 0:
            draw_sprite(spr_dialog, 3, tX + width + padding, tY + 8 + 8*j);
            draw_sprite(spr_dialog, 7, tX, tY + 8 + 8*j);
        break;
        case 1:
            draw_sprite(spr_menu_dialog, 3, tX + width + padding, tY + 8 + 8*j);
            draw_sprite(spr_menu_dialog, 7, tX, tY + 8 + 8*j);
        break;
        case 2:
            draw_sprite(spr_exclamation_dialog, 3, tX + width + padding, tY + 8 + 8*j);
            draw_sprite(spr_exclamation_dialog, 7, tX, tY + 8 + 8*j);
        break;
    }
 
}

//Draw fill rectangle
draw_rectangle(tX + 8, tY + 8, tX + width + padding, tY + height + padding, false);

//Draw point if target exists
if (target != noone) {

    draw_sprite(spr_dialog_point, box, tX + (width / 2) + padding, tY + height + padding + 7);
}

//Set the font if we didn't
draw_set_font(fnt_dialog);

//If the text should be displayed
if (showing) {   
    
    //Increment the number of letters that should be displayed
    if (a &lt; string_length(text)) {
    
        a++;
    }

    //Draw the text up to the number of letters that should be displayed
    switch (box)
    {
        case 0:
            draw_text_colour_shadowed(tX + padding, tY + padding, string_copy(text, 1, a), c_black, c_black, 1, 1, 0.5, 1);
        break;
        case 1:
            draw_text_colour_shadowed(tX + padding, tY + padding, string_copy(text, 1, a), c_white, c_white, 1, 1, 0.5, 1);
        break;
        case 2:
            draw_text_colour_shadowed(tX + padding, tY + padding, string_copy(text, 1, a), c_black, c_black, 1, 1, 0.5, 1);
        break;
    }

    
    //Draw text end/continue arrows.
    if (a &gt;= string_length(text)) {
    
        if audio_is_playing(snd_dialog)
            audio_stop_sound(snd_dialog);
            
        if (newtext = "")
            draw_sprite(spr_dialog_end,anim, tX + width + padding - 4, tY + height + padding - 4);
        else
            draw_sprite(spr_dialog_continue,anim, tX + width + padding - 4, tY + height + padding - 4);
    }
}
</string>
          </argument>
        </arguments>
      </action>
