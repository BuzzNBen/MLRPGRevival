        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Advance the text
if (gamepad_button_check(0, global.buttonA) || keyboard_check(global.keyA))
{

    //If text is being displayed
    if (showing) {
    
        //If the full message is not displayed yet, display it all.
        if (a &lt; string_length(text))
        {  
            a = string_length(text)+1;
            if audio_is_playing(snd_dialog)
                audio_stop_sound(snd_dialog);
        }
        
        //Otherwise
        else {
    
            //If there is more to be displayed
            if (newtext != "") {
    
                //Make the new text the current text
                text = newtext;
                newtext = "";
                
                //Play dialog sound
                if !audio_is_playing(snd_dialog)
                    audio_play_sound(snd_dialog,0,1);
    
                //Split up the message if it is too long
                event_user(0);
            }
    
            //Otherwise, end dialog
            else 
                event_user(1);
        }
    }

}
</string>
