        {
            if global.LBP &lt; 4 or global.brosattacks = 0 or brositemdifmax = 0 or global.Mdead &gt; 0
                audio_play_sound(snd_incorrect,0,0);
            else if movechoose = 1 &amp;&amp; global.brosattacks &gt;= 1 &amp;&amp; global.LBP &gt;= 4
            {
                //Set Bros Attack to Green Shells
