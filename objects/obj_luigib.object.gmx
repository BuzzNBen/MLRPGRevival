    {
        sprite_index = spr_luigichoosejump;
        zspeed = 5;
        jumpchoosewait = 6;
        jumpchoosego = 1;
        control = 0;
        jumpingchoose = 1;
    }

    if phase = 2 &amp;&amp; battleblock &lt;&gt; 3 //Choose an enemy
    {
