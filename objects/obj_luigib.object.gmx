                stance = 0; //stance 0 means that he is not in a stance
                control = 0;
            }
            audio_play_sound(snd_confirm,0,0);
        }
        if battleblock = 1 //Bros
        {
            alarm[8] = 20;
            phase = 0.25;
            zspeed = 0;
            stance = 0; //stance 0 means that he is not in a stance
            control = 0;
            audio_play_sound(snd_confirm,0,0);
        }
        if battleblock = 3 //Items
        {
            alarm[8] = 20; //Buffer
